{
    "cSpell.words": [
        "acumin",
        "babylonjs",
        "fontawesome",
        "fortawesome",
        "minifiers",
        "multilines",
        "runtimes",
        "Sebastien",
        "smartfilter",
        "tsbuildinfo",
        "tsdoc",
        "VANDENBERGHE",
